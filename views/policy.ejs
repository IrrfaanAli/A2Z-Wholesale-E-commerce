            <span class="ml-6 text-sm font-bold text-black">GEROCERIES</span>
          </a>
          <a
            href="/sports_item"
            class="flex items-center px-6 py-3 transition hover:bg-gray-100"
          >
            <span class="ml-6 text-sm font-bold text-black"
              >SPORTS ITEMS</span
            >
          </a>
          <a
            href="/kitchen'"
            class="flex items-center px-6 py-3 transition hover:bg-gray-100"
          >
            <span class="ml-6 text-sm font-bold text-black">KITCHEN</span>
          </a>
          <a
            href="/tools"
            class="flex items-center px-6 py-3 transition hover:bg-gray-100"
          >
            <span class="ml-6 text-sm font-bold text-black">TOOLS</span>
          </a>
          <a
            href="/baby_items"
            class="flex items-center px-6 py-3 transition hover:bg-gray-100"
          >
            <span class="ml-6 text-sm font-bold text-black">BABY ITEMS</span>
          </a>
          <a
            href="jewelery"
            class="flex items-center px-6 py-3 transition hover:bg-gray-100"
          >
            <span class="ml-6 text-sm font-bold text-black">JEWELERY</span>
          </a>
        </div>
      </div>

      <!--link start-->
      <div class="flex flex-grow items-center justify-between pl-12">
        <div class="flex items-center space-x-6 capitalize">
          <a href="/home" class="text-white transition hover:text-white"
            >Home</a
          >
          <a href="/shop" class="text-white transition hover:text-white"
            >Shop</a
          >
          <a
            href="/order_tracking"
            class="text-white transition hover:text-white"
            >Order Tracking</a
          >
          <a href="/refund" class="text-white transition hover:text-white"
            >Exchange/Refund</a
          >
          <a href="/information" class="text-white transition hover:text-white"
            >Information</a
          >
        </div>
        <a href="/login" class="mr-16 text-white">Login/Register</a>
      </div>

      <!--link end-->
    </div>
  </nav>
