    <!-- add start-->

    <div class="container pb-16 ml-6 mb-16 mr-6">
       <h2 class="tex-gray-800 mb-6 ml-6 mt-6 text-2xl font-medium uppercase">
        Contat Us
      </h2>

      <div >
      <div>
         <img src="image/contact.jpg" class="ml-6 h-44" />

